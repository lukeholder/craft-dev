{
  "require": {
    "craftcms/cms": "^4.9.5",
    "vlucas/phpdotenv": "^5.5.0",
    "verbb/navigation": "^2.0.23",
    "ether/simplemap": "^4.0.4",
    "topshelfcraft/wordsmith": "^4.4.0",
    "topshelfcraft/supersort": "^4.0.0",
    "craftcms/element-api": "^4.1.0",
    "matt-west/craft-recaptcha": "^2.0.2",
    "solspace/craft-freeform": "^5.3.1",
    "craftcms/feed-me": "^5.5.0",
    "venveo/craft-bulkedit": "^4.0.1",
    "nystudio107/craft-seomatic": "^4.0.37",
    "craftcms/azure-blob": "^2.0.0",
    "topshelfcraft/new-relic": "^4.0.1",
    "solspace/craft-calendar": "^4.0.11",
    "perfectwebteam/craft-mailchimp-transactional": "^2.1.0",
    "boxhead/craft-country-select-field": "^4.0",
    "mailchimp/marketing": "^3.0",
    "weareferal/remote-sync": "dev-disable-restore",
    "aws/aws-sdk-php": "^3.300",
    "craftcms/ckeditor": "^3.8.3",
    "craftcms/cloud": "^1.50.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\sitemodule\\": "modules/sitemodule/src/",
      "modules\\teentixcsp\\": "modules/teentixcsp/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.13"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true,
      "kylekatarnls/update-helper": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    },
    {
      "type": "git",
      "url": "https://github.com/teentix/craft-remote-sync"
    }
  ]
}
